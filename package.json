{
  "name": "react-state-selector",
  "version": "1.0.9",
  "keywords": [
    "react",
    "immer",
    "immerjs",
    "redux",
    "state",
    "reselect",
    "selector",
    "react-state"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/react-state-selector.git"
  },
  "license": "MIT",
  "author": "PabloSzx",
  "main": "dist/index.js",
  "module": "dist/react-state-selector.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build -i ./src/index.ts",
    "build-storybook": "rm -rf ./dist && build-storybook -o docs  -s stories/utils/public",
    "lint": "tsdx lint src stories test --fix",
    "prepublishOnly": "tsdx lint src test --fix && tsdx test && tsdx build",
    "start": "tsdx watch",
    "storybook": "start-storybook -p 6006 -s stories/utils/public",
    "test": "tsdx test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && tsdx lint --fix src stories test"
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/"
  },
  "resolutions": {
    "@types/react": "^16.9.25",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "immer": "^7.0.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
